@model ItemViewModel[]
<h2>Table of Items</h2>
<table class="table">
	<tr>
		<td>Id</td>
		<td>Name</td>
		<td>Count</td>
		<td>Country</td>
		<td>Category</td>
	</tr>
	@foreach(var item in Model)
	{
		<tr>
			<td>@item.Id</td>
			<td>@item.Name</td>
			<td>@item.Count</td>
			<td>@item.Country</td>
			<td>@item.Category</td>
		</tr>
	}
</table>

